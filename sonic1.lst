00001830                                move.b  (a0)+,d0                ; Get desc field low-byte.
00001830                                move.b  (a0)+,d1                ; Get desc field hi-byte.
00001830                                if Kos_UseLUT=1
00001830                                move.b  (a4,d0.w),d0            ; Invert bit order...
00001830                                move.b  (a4,d1.w),d1            ; ... for both bytes.

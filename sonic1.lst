0000131C 6100 0000                      bsr.w   NemDec_BuildCodeTable
00001320 1A18                           move.b  (a0)+,d5        ; get first byte of compressed data
00001322 E145                           asl.w   #8,d5           ; shift up by a byte
00001324 1A18                           move.b  (a0)+,d5        ; get second byte of compressed data
00001326 3C3C 0010                      move.w  #$10,d6         ; set initial shift value
0000132A 6100                           bsr.s   NemDec2

00001306 43F8 AA00                      lea $FFFFAA00,a1        ; load Nemesis decompression buffer
0000130A 3418                           move.w  (a0)+,d2        ; get number of patterns
0000130C 6A00                           bpl.s   @0          ; are we in Mode 0?
0000130E 47EB 000A                      lea $A(a3),a3       ; if not, use Mode 1
00001312 E74A                       @0  lsl.w   #3,d2
00001314 3A42                           movea.w d2,a5
00001316 7607                           moveq   #7,d3
00001318 7400                           moveq   #0,d2

0000C804                            AnimateSprite:
0000C804 7000                       		moveq	#0,d0
0000C806 1028 001C                  		move.b	obAnim(a0),d0	; move animation number	to d0
0000C80A B028 001D                  		cmp.b	obNextAni(a0),d0 ; is animation set to restart?
